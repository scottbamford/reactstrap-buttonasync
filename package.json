{
  "name": "reactstrap-buttonasync",
  "version": "1.0.1",
  "description": "Reactstrap Button with support that will disable itself and optionally show different children when isExecuting is true (usually during an async task).",
  "main": "dist/lib/commonjs/index.js",
  "module": "dist/lib/esm/index.js",
  "types": "dist/lib/esm/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Scott Bamford",
    "email": "scott@ambidect.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottbamford/reactstrap-buttonasync.git"
  },
  "keywords": [
    "typescript",
    "react",
    "async",
    "callback",
    "button",
    "disabled",
    "wait"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/scottbamford/reactstrap-buttonasync/issues"
  },
  "homepage": "https://github.com/scottbamford/reactstrap-buttonasync/#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc && tsc -m esnext --outDir ./dist/lib/esm",
    "rebuild": "npm run-script clean && npm run-script build"
  },
  "dependencies": {
    "react": "^16.8.6",
    "reactstrap": "^8.0.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/react": "^16.8.16",
    "@types/reactstrap": "^8.0.1",
    "typescript": "3.4.5",
    "rimraf": "^2.6.3"
  }
}
